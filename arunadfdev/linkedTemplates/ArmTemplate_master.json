{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "arunadfdev"
		},
		"LinkedService_ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LinkedService_ADLS'"
		},
		"LinkedService_AzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkedService_AzureSqlDatabase'"
		},
		"LinkedService_Snowflake_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkedService_Snowflake'"
		},
		"LinkedService_ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://arunadlsdev001.dfs.core.windows.net"
		},
		"LinkedService_AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://arunakvdev.vault.azure.net/"
		},
		"LinkedService_AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://arunakvdev.vault.azure.net/"
		},
		"LinkedService_BlobStorage_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'LinkedService_BlobStorage'"
		},
		"LinkedService_RESTAPI_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gorest.co.in/public/v2/users"
		},
		"LinkedService_RESTAPI_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin"
		},
		"LinkedService_StorageAccount_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'LinkedService_StorageAccount'"
		},
		"Snowflake1_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "SF-CONNECTION-STRING"
		},
		"trigger1_properties_SF_StoredProcedure_parameters_EmpNmae": {
			"type": "string",
			"defaultValue": "dhddjjd"
		},
		"trigger1_properties_SF_StoredProcedure_parameters_EmpLocation": {
			"type": "string",
			"defaultValue": "dhdjdjd"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "arunadfdev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LinkedService_ADLS_accountKey": {
						"value": "[parameters('LinkedService_ADLS_accountKey')]"
					},
					"LinkedService_AzureSqlDatabase_connectionString": {
						"value": "[parameters('LinkedService_AzureSqlDatabase_connectionString')]"
					},
					"LinkedService_Snowflake_connectionString": {
						"value": "[parameters('LinkedService_Snowflake_connectionString')]"
					},
					"LinkedService_ADLS_properties_typeProperties_url": {
						"value": "[parameters('LinkedService_ADLS_properties_typeProperties_url')]"
					},
					"LinkedService_AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LinkedService_AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"LinkedService_AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('LinkedService_AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"LinkedService_BlobStorage_sasUri": {
						"value": "[parameters('LinkedService_BlobStorage_sasUri')]"
					},
					"LinkedService_StorageAccount_sasUri": {
						"value": "[parameters('LinkedService_StorageAccount_sasUri')]"
					},
					"LinkedService_RESTAPI_properties_typeProperties_url": {
						"value": "[parameters('LinkedService_RESTAPI_properties_typeProperties_url')]"
					},
					"LinkedService_RESTAPI_properties_typeProperties_userName": {
						"value": "[parameters('LinkedService_RESTAPI_properties_typeProperties_userName')]"
					},
					"Snowflake1_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('Snowflake1_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "arunadfdev_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"trigger1_properties_SF_StoredProcedure_parameters_EmpNmae": {
						"value": "[parameters('trigger1_properties_SF_StoredProcedure_parameters_EmpNmae')]"
					},
					"trigger1_properties_SF_StoredProcedure_parameters_EmpLocation": {
						"value": "[parameters('trigger1_properties_SF_StoredProcedure_parameters_EmpLocation')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'arunadfdev_ArmTemplate_0')]"
			]
		}
	]
}